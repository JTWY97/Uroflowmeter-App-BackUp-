<FluidIntake>:
    FloatLayout:
        MDLabel:
            id: Symptom_T
            text: "How much did you drink!"
            font_size: 40
            halign: 'center'
            size_hint_y: None
            pos_hint: {"top": .9, "center_x": .5}
            height: self.texture_size[1]  

        MDIconButton: 
            id: ButtonOne
            icon: 'bottle-soda-classic-outline'
            pos_hint: {"top":0.7, "center_x": 0.3}
            theme_text_color: 'Custom'
            text_color: app.theme_cls.primary_color
            on_release:
                root.callback(button = 'button1')

#medium button
        MDIconButton:
            icon: 'bottle-soda-classic-outline'
            pos_hint: {"top":0.7, "center_x": 0.5}
            theme_text_color: 'Custom'
            text_color: app.theme_cls.primary_color
            user_font_size: "48sp"
            on_release:
                root.callback(button = 'button2')

#Large button
        MDIconButton:
            icon: 'bottle-soda-classic-outline'
            pos_hint: {"top":0.7, "center_x": 0.7}
            theme_text_color: 'Custom'
            text_color: app.theme_cls.primary_color
            user_font_size: "64sp"
            on_release:
                root.callback(button = 'button3')

#Delete Last Log
        MDRoundFlatButton:
            text: "Delete last entry"
            font_size: 25
            size_hint: .5, .06
            pos_hint: {"top":0.6, "center_x": 0.5}
            on_release:
                root.callback(button = 'button4')

        TextInput:
            text: "Volume here!"
            id: volumeop
            multiline: False
            readonly: True
            font_size: 43
            size_hint: (1,0.1)
            pos_hint: {"top":0.4, "center_x": 0.5}

        MDRoundFlatButton:
            text: "Back"
            font_size: 25
            size_hint: .5, .06
            pos_hint: {"top":0.5, "center_x": 0.5}
            on_release:
                root.parent.current = "Main"
                root.manager.transition.direction = "right"
        Widget:
            size_hint_y: None
            height: 10