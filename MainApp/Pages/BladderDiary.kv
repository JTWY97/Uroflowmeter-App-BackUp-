<BladderDiary>:
    ScrollView:
        do_scroll_x: False
        do_scroll_y: True
        GridLayout:
            rows: 4
            size:(root.width, root.height)
            size_hint_y: None
            height: self.minimum_height
            row_default_height: 180
            #row_force_default: True 
            MDList:
                id: BladderDiary
            MDList:
                id: BladderSummary

            MDRoundFlatButton:
                text: "Access Symptoms Here"
                on_press:
                    root.parent.current = "SymptomTracker"

            MDRoundFlatButton:
                text: "Back"
                on_press:
                    root.parent.current = "PatientHomePage"