<PatientReport>:
    ScrollView:
        do_scroll_x: False
        do_scroll_y: True
        FloatLayout:
            BoxLayout:
                orientation: "vertical"
                pos_hint: {"top":1, "center_x": 0.5}
                Image:
                    id: imageView
                    source: './Styles/Patient.png'
                    pos_hint: {"top":1, "center_x": 0.5}
                    size_hint: .7, 0.9
                    allow_stretch: True
            MDRoundFlatButton:
                text: "Back"
                font_size: 25
                size_hint: .7, 0.06
                pos_hint: {"top":0.1, "center_x": 0.5}
                on_release:
                    root.parent.current = "PatientHomePage"
                    root.manager.transition.direction = "right"