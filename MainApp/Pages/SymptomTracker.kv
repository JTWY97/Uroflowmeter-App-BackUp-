<SymptomTracker>:
    id:SymptomTracker
    name: 'SymptomTracker'
    ScrollView:
        do_scroll_x: False
        do_scroll_y: True

        GridLayout:
            rows: 9
            cols: 1
            size:(root.width, root.height)
            size_hint_y: None
            height: self.minimum_height
            row_default_height: 70
            # row_force_default: True 

            FloatLayout:
                ThreeLineIconListItem:
                    id: VoidInfo
                    text: "Void Time:"
                    secondary_text: "Void Type:"
                    tertiary_text: "Void Volume:"
                    font_size: 30
                    halign: 'center'
                    size_hint_y: None
                    pos_hint: {"top": 1, "center_x": .5}
                    IconLeftWidget:
                        id: VoidInfoIcon
                        icon:"./Styles/BladderDiaryIcons/Normal.png"

                MDLabel:
                    text: "Change void type:"
                    font_size: 25
                    halign: 'center'
                    size_hint_y: None
                    pos_hint: {"top": .1, "center_x": .5}
                    height: self.texture_size[1]

            FloatLayout:
                MDLabel:
                    text: "Morning Void"
                    font_size: 20
                    halign: 'center'
                    size_hint_y: None
                    pos_hint: {"top":0.3, "center_x": 0.2}
                    height: self.texture_size[1]
                MDIconButton:
                    id: Morning
                    pos_hint: {"top":0.8, "center_x": 0.2}
                    icon: "./Styles/BladderDiaryIcons/Morning.png"
                    user_font_size: dp(40)
                    on_press:
                        root.ChangeVoidType(button = 'Morning')

                MDLabel:
                    text: "Normal Void"
                    font_size: 20
                    halign: 'center'
                    size_hint_y: None
                    pos_hint: {"top":0.3, "center_x": 0.5}
                    height: self.texture_size[1]
                MDIconButton:
                    id: Normal
                    pos_hint: {"top":0.8, "center_x": 0.5}
                    icon: "./Styles/BladderDiaryIcons/Normal.png"
                    user_font_size: dp(40)
                    on_press:
                        root.ChangeVoidType(button = 'Normal')

                MDLabel:
                    text: "Nocturia Void"
                    font_size: 20
                    halign: 'center'
                    size_hint_y: None
                    pos_hint: {"top":0.3, "center_x": 0.8}
                    height: self.texture_size[1]
                MDIconButton:
                    id: Nocturia
                    pos_hint: {"top":0.8, "center_x": 0.8}
                    icon: "./Styles/BladderDiaryIcons/Nocturia.png"
                    user_font_size: dp(40)
                    on_press:
                        root.ChangeVoidType(button = 'Nocturia')


            FloatLayout:
                MDLabel:
                    id: Symptom_T
                    text: "What symptoms did you experience during this void?"
                    font_size: 25
                    halign: 'center'
                    size_hint_y: None
                    pos_hint: {"top": .9, "center_x": .5}
                    height: self.texture_size[1]  

            GridLayout:
                rows: 3
                cols: 5
                height: self.minimum_height
                # size_hint_y: None
                pos_hint: {"center_x": .5}
                MDIconButton:
                    id: ButtonOne
                    icon: "./Styles/SymptomTrackerIcons/Fatigue.png"
                    theme_text_color: 'Custom'
                    text_color: app.theme_cls.primary_color
                    user_font_size: 90
                    on_release:
                        root.SendSymptom(button = 'button1')
                MDIconButton:
                    id: ButtonTwo
                    icon: "./Styles/SymptomTrackerIcons/SwollenLeg.png"
                    theme_text_color: 'Custom'
                    text_color: app.theme_cls.primary_color
                    user_font_size: 90
                    on_release:
                        root.SendSymptom(button = 'button2')
                MDIconButton:
                    id: ButtonThree
                    icon: "./Styles/SymptomTrackerIcons/Fever.png"
                    theme_text_color: 'Custom'
                    text_color: app.theme_cls.primary_color
                    user_font_size: 90
                    on_release:
                        root.SendSymptom(button = 'button3')
                MDIconButton:
                    id: ButtonFour
                    icon: "./Styles/SymptomTrackerIcons/Nausea.png"
                    theme_text_color: 'Custom'
                    text_color: app.theme_cls.primary_color
                    user_font_size: 90
                    on_release:
                        root.SendSymptom(button = 'button4')
                MDIconButton:
                    id: ButtonFive
                    icon: "./Styles/SymptomTrackerIcons/Headache.png"
                    theme_text_color: 'Custom'
                    text_color: app.theme_cls.primary_color
                    user_font_size: 90
                    on_release:
                        root.SendSymptom(button = 'button5')
                MDIconButton:
                    id: ButtonSix
                    icon: "./Styles/SymptomTrackerIcons/Leaks.png"
                    pos_hint: {"top":1, "center_x": 0.3}
                    theme_text_color: 'Custom'
                    text_color: app.theme_cls.primary_color
                    user_font_size: 90
                    on_release:
                        root.SendSymptom(button = 'button6')
                MDIconButton:
                    id: ButtonSeven
                    icon: "./Styles/SymptomTrackerIcons/Foulsmell.png"
                    theme_text_color: 'Custom'
                    text_color: app.theme_cls.primary_color
                    user_font_size: 90
                    on_release:
                        root.SendSymptom(button = 'button7')
                MDIconButton:
                    id: ButtonEight
                    icon: "./Styles/SymptomTrackerIcons/pelvicpain.png"
                    theme_text_color: 'Custom'
                    text_color: app.theme_cls.primary_color
                    user_font_size: 90
                    on_release:
                        root.SendSymptom(button = 'button8')
                MDIconButton:
                    id: ButtonNine
                    icon: "./Styles/SymptomTrackerIcons/cloudyurine.png"
                    theme_text_color: 'Custom'
                    text_color: app.theme_cls.primary_color
                    user_font_size: 90
                    on_release:
                        root.SendSymptom(button = 'button9')
                MDIconButton:
                    id: ButtonTen
                    icon: "./Styles/SymptomTrackerIcons/crampsonside.png"
                    theme_text_color: 'Custom'
                    text_color: app.theme_cls.primary_color
                    user_font_size: 90
                    on_release:
                        root.SendSymptom(button = 'button10')
                MDIconButton:
                    id: ButtonEleven
                    icon: "./Styles/SymptomTrackerIcons/hesitancytourinate.png"
                    theme_text_color: 'Custom'
                    text_color: app.theme_cls.primary_color
                    user_font_size: 90
                    on_release:
                        root.SendSymptom(button = 'button11')

            FloatLayout:
                MDRoundFlatButton:
                    text: "Back"
                    pos_hint: {"x":0.45, "y":-2.5}
                    on_release:
                        root.parent.current = "PatientHomePage"
                        # root.manager.transition.direction = "right"