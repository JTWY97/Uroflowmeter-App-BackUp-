<SymptomTracker>:
    id:SymptomTracker
    name: 'SymptomTracker'
    ScrollView:
        do_scroll_x: False
        do_scroll_y: True

        GridLayout:
            rows: 8
            cols: 1
            size:(root.width, root.height)
            size_hint_y: None
            height: self.minimum_height
            row_default_height: 70
            pos_hint: {"center_x": .5}
            # row_force_default: True 

            FloatLayout:
                ThreeLineIconListItem:
                    id: VoidInfo
                    text: "Void Time:"
                    secondary_text: "Void Type:"
                    tertiary_text: "Void Volume:"
                    font_size: 30
                    halign: 'center'
                    size_hint_y: None
                    pos_hint: {"top": 1, "center_x": .5}
                    IconLeftWidget:
                        id: VoidInfoIcon
                        icon:"./Styles/BladderDiaryIcons/Normal.png"

                MDLabel:
                    text: "Change void type:"
                    font_size: 25
                    halign: 'center'
                    size_hint_y: None
                    pos_hint: {"top": -0.2, "center_x": .5}
                    height: self.texture_size[1]

            FloatLayout:
                MDLabel:
                    text: "Morning Void"
                    font_size: 20
                    halign: 'center'
                    size_hint_y: None
                    pos_hint: {"top":-0.2, "center_x": 0.2}
                    height: self.texture_size[1]
                MDIconButton:
                    id: Morning
                    pos_hint: {"top":0.55, "center_x": 0.2}
                    icon: "./Styles/BladderDiaryIcons/Morning.png"
                    user_font_size: dp(40)
                    on_press:
                        root.ChangeVoidType(button = 'Morning')

                MDLabel:
                    text: "Normal Void"
                    font_size: 20
                    halign: 'center'
                    size_hint_y: None
                    pos_hint: {"top":-0.2, "center_x": 0.5}
                    height: self.texture_size[1]
                MDIconButton:
                    id: Normal
                    pos_hint: {"top":0.55, "center_x": 0.5}
                    icon: "./Styles/BladderDiaryIcons/Normal.png"
                    user_font_size: dp(40)
                    on_press:
                        root.ChangeVoidType(button = 'Normal')

                MDLabel:
                    text: "Nocturia Void"
                    font_size: 20
                    halign: 'center'
                    size_hint_y: None
                    pos_hint: {"top":-0.2, "center_x": 0.8}
                    height: self.texture_size[1]
                MDIconButton:
                    id: Nocturia
                    pos_hint: {"top":0.55, "center_x": 0.8}
                    icon: "./Styles/BladderDiaryIcons/Nocturia.png"
                    user_font_size: dp(40)
                    on_press:
                        root.ChangeVoidType(button = 'Nocturia')


            FloatLayout:
                MDLabel:
                    id: Symptom_T
                    text: "What symptoms did you experience during this void?"
                    font_size: 25
                    halign: 'center'
                    size_hint_y: None
                    pos_hint: {"top": .45, "center_x": .5}
                    height: self.texture_size[1]  

            FloatLayout:
                MDIconButton:
                    id: ButtonOne
                    icon: "./Styles/SymptomTrackerIcons/Fatigue.png"
                    pos_hint: {"top": 1.4, "center_x": .1}
                    theme_text_color: 'Custom'
                    text_color: app.theme_cls.primary_color
                    user_font_size: dp(110)
                    on_release:
                        root.SendSymptom(button = 'button1')
                MDLabel:
                    text: "Fatigue"
                    font_size: 20
                    halign: 'left'
                    size_hint_y: None
